  "description": "LeCuay's Little Garden.",
  "main": "src/index.ts",
  "scripts": {
    "start": "webpack-dev-server --config ./config/webpack.config.js --progress --config-name main --config-name dev --merge",
    "build": "if-env NODE_ENV=production ?? npm run build:prod || npm run build:dev",
    "build:prod": "webpack --config ./config/webpack.config.js --config-name main --config-name prod --merge",
    "build:dev": "webpack --config ./config/webpack.config.js --config-name main --config-name dev --merge",
    "build:watch": "npm run build:dev -- --watch",
    "lint": "eslint --color .",
    "lint:fix": "eslint --fix .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeCuay/lecuay.github.io.git"
  },
