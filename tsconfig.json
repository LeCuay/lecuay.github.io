{
  "compilerOptions": {
    "target": "ES3",
    "lib": ["DOM", "ES2020"],
    "jsx": "react",
    "module": "ESNext",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "./vendor/",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "baseUrl": "src"
  },
  "include": ["src"]
}
